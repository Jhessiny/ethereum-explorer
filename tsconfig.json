{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "DOM.Iterable", "ESNext"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "allowJs": true,
    "strict": true,
    "resolveJsonModule": true,
    "strictFunctionTypes": false
  },
  "include": ["src"]
}
